{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": false,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"module": "ESNext",
		"moduleResolution": "Node",
		"paths": {
			"$lib/*": ["./src/lib/*"]
		}
	}
}
